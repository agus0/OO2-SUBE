<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="datos.Beneficio" table="beneficio">
			<id name="idBeneficio" column="idBeneficio">
		<generator class="foreign">
			<param name="property">usuario</param>
		</generator>
		</id>
		<property column="beneficio" name="beneficio" type="string"/>
		
		<one-to-one name="usuario" class="datos.Usuario"></one-to-one>
		
	<joined-subclass name="datos.Descuento" table="descuento">
	<key column="idDescuento"></key>
	<property column="porcentajeDescuento" name="porcentajeDescuento" type="double"/>
	</joined-subclass>
	<joined-subclass name="datos.ViajeGratis" table="viajeGratis">
	<key column="idViajeGratis"></key>
	<property column="cantidad" name="cantidad" type="int"/>
	</joined-subclass>
	<joined-subclass name="datos.MontoFijo" table="montoFijo">
	<key column="idMontoFijo"></key>
	<property column="monto" name="monto" type="double"/>
	</joined-subclass>
	</class>
</hibernate-mapping>