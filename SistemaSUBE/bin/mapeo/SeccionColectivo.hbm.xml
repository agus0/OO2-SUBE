<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="datos.SeccionColectivo" table="seccionColectivo">
		<id column="idSeccionColectivo" name="idSeccionColectivo">
			<generator class="identity"/>
		</id>
		<property column="precioSeccionColectivo" name="precioSeccionColectivo" type="double"/>
		
		<set table="tramoColectivo" name="lectorColectivo" outer-join="true">
			<key column="idSeccionColectivo" />
			<many-to-many column="idLectorColectivo" class="datos.LectorColectivo" />
		</set>
	</class>
</hibernate-mapping>