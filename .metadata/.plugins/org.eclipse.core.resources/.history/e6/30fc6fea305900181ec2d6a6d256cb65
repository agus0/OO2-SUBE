package test;

import datos.Usuario;
import negocio.BoletoEstudiantilABM;
import negocio.UsuarioABM;

public class TestAgregarBoletoEstudiantil {
	public static void main(String[] args) throws Exception {
		String beneficio= "Plan Estudiantes" ;
		double montoFijo = 6.15;
		UsuarioABM uabm = UsuarioABM.getInstanciaUsuarioABM();
		Usuario usuario = uabm.traerI(1);
		BoletoEstudiantilABM dabm = BoletoEstudiantilABM.getInstanciaMontoFijoABM();
		dabm.agregar(beneficio,usuario,montoFijo);

	}
}
